{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "linq2db/Source/LinqToDB/LinqToDB.csproj",
            "linq2db/Source/LinqToDB.Tools/LinqToDB.Tools.csproj",
            "linq2db.EntityFrameworkCore/Source/LinqToDB.EntityFrameworkCore/linq2db.EntityFrameworkCore.csproj",
            "LinqToDB.Identity/src/LinqToDB.Identity/LinqToDB.Identity.csproj",
            "IdentityServer4.LinqToDB/src/IdentityServer4.LinqToDB/IdentityServer4.Contrib.Linq2db.csproj"
          ],
          "src": "../submodules"
        }
      ],
      "dest": "./api",
      "filter": "filterConfig.yml",      
      "properties": {
        "TargetFramework": "net45"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "articles/**/*.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**.png",
          "**.jpg"
        ]
      }
    ],
    "dest": "../_site",
    "globalMetadataFiles": [],
    "xref": [
      "../tools/packages/msdn.4.5.2/content/msdn.4.5.2.zip"
    ],
    // "xrefService": [
    //   "https://xref.docs.microsoft.com/query?uid={uid}"
    // ],
    "globalMetadata": {
      "_appTitle": "Linq To DB (aka linq2db)",
      "_appFooter": "Copyright Â© 2011-2020 linq2db.com<br><br>Generated by <strong>DocFX</strong>",
      "_enableSearch": true,
      "_appLogoPath":"images/icon.png",
      "_appFaviconPath": "images/icon.ico"
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false
  }
}